{
  "expo": {
    "name": "Predict",
    "slug": "Predict",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "predict",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.weewex.predict"
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "edgeToEdgeEnabled": true,
      "package": "com.weewex.predict"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#0B0B10",
          "dark": {
            "backgroundColor": "#141420"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "158ef8dd-64f2-41e2-a485-4d899f21d24e"
      },
      "game": {
        "genre": "casual",
        "theme": "target-guessing",
        "developer": "WeeWex Games"
      }
    }
  }
}
